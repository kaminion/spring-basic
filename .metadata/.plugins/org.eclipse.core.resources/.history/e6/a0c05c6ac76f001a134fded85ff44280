package t_tok2;

public class BlackPhone implements Phone{
	
	private Battery battery;
	
	public BlackPhone()
	{
		this.battery = new Battery();
	}
	
	public BlackPhone(Battery battery)
	{
		this.battery = battery;
	}
	
	public void setBattery(Battery battery)
	{
		this.battery = battery;
	}
	
	
	public void show()
	{
		
	}
}
